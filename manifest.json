{
  "manifest_version": 2,

  "name": "Frizzle",
  "description": "Saving humans time!",
  "version": "1.0",

  "browser_action": {
    "default_icon"  : "icon.png",
    "default_popup" : "popup.html"
  },
    "content_scripts" : [
    {
      "matches" : ["<all_urls>"],
      "css"     : ["css/page.css"
                  ],
      "js"      : ["messaging.js",
                  ]
	}],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
